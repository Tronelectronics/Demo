@model IEnumerable<Demo.Log>

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <link crossorigin="anonymous" href="https://assets-cdn.github.com/assets/frameworks-a4bf54bef6fb.css" rel="stylesheet" />
    <link crossorigin="anonymous" href="https://assets-cdn.github.com/assets/github-ac2ce2821692.css" rel="stylesheet" />
    <title></title>
</head>
<body>
    <div>
        <div>
            <div id="js-repo-pjax-container">
                <div class="container new-discussion-timeline experiment-repo-nav">
                    <div class="repository-content">
                        <div class="commit-tease js-details-container Details">
                            <span class="float-right" style="font-weight:bold;">
                                @{
                                    int total = Model.Count();
                                }
                                Totally <span>@Html.DisplayFor(modelItem => total)</span> Records
                            </span>
                            <div>
                                <span style="font-weight:bold;">Logs</span>
                            </div>
                        </div>
                        <div class="file-wrap">
                            <table class="files js-navigation-container js-active-navigation-container">
                                <tbody>
                                    <tr class="js-navigation-item">
                                        <td class="message">
                                            <span class="css-truncate css-truncate-target">@Html.ActionLink("Create New", "Create")</span>
                                        </td>
                                    </tr>
                                    <tr class="js-navigation-item">
                                        <td class="message">
                                            <span class="css-truncate css-truncate-target">@Html.DisplayNameFor(model => model.Time)</span>
                                        </td>
                                        <td class="message">
                                            <span class="css-truncate css-truncate-target">@Html.DisplayNameFor(model => model.City)</span>
                                        </td>
                                        <td class="message">
                                            <span class="css-truncate css-truncate-target">@Html.DisplayNameFor(model => model.Country)</span>
                                        </td>
                                        <td class="message">
                                            <span class="css-truncate css-truncate-target">@Html.DisplayNameFor(model => model.Description)</span>
                                        </td>
                                        <td class="message">
                                            <span class="css-truncate css-truncate-target">@Html.DisplayNameFor(model => model.District)</span>
                                        </td>
                                        <td class="message">
                                            <span class="css-truncate css-truncate-target">@Html.DisplayNameFor(model => model.Ip)</span>
                                        </td>
                                        <td class="message">
                                            <span class="css-truncate css-truncate-target">@Html.DisplayNameFor(model => model.Isp)</span>
                                        </td>
                                        <td class="message">
                                            <span class="css-truncate css-truncate-target">@Html.DisplayNameFor(model => model.Province)</span>
                                        </td>
                                        <td class="message">
                                            <span class="css-truncate css-truncate-target">@Html.DisplayNameFor(model => model.Type)</span>
                                        </td>
                                        <td class="message"></td>
                                    </tr>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td class="message">
                                                @{
                                                    string time = item.Time.HasValue ? item.Time.Value.ToString("yyyy-MM-dd HH:mm:ss") : "";
                                                }
                                                <span class="css-truncate css-truncate-target">@Html.DisplayFor(modelItem => time)</span>
                                            </td>
                                            <td class="message">
                                                <span class="css-truncate css-truncate-target">@Html.DisplayFor(modelItem => item.City)</span>
                                            </td>
                                            <td class="message">
                                                <span class="css-truncate css-truncate-target">@Html.DisplayFor(modelItem => item.Country)</span>
                                            </td>
                                            <td class="message">
                                                <span class="css-truncate css-truncate-target">@Html.DisplayFor(modelItem => item.Description)</span>
                                            </td>
                                            <td class="message">
                                                <span class="css-truncate css-truncate-target">@Html.DisplayFor(modelItem => item.District)</span>
                                            </td>
                                            <td class="message">
                                                <span class="css-truncate css-truncate-target">@Html.DisplayFor(modelItem => item.Ip)</span>
                                            </td>
                                            <td class="message">
                                                <span class="css-truncate css-truncate-target">@Html.DisplayFor(modelItem => item.Isp)</span>
                                            </td>
                                            <td class="message">
                                                <span class="css-truncate css-truncate-target">@Html.DisplayFor(modelItem => item.Province)</span>
                                            </td>
                                            <td class="message">
                                                <span class="css-truncate css-truncate-target">@Html.DisplayFor(modelItem => item.Type)</span>
                                            </td>
                                            <td class="message">
                                                <span class="css-truncate css-truncate-target">
                                                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                                                    @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                                                    @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                                                </span>
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                        <div id="readme" class="readme boxed-group clearfix announce instapaper_body md">
                            <h3>
                                <svg aria-hidden="true" class="octicon octicon-book" height="16" version="1.1" viewBox="0 0 16 16" width="16"><path fill-rule="evenodd" d="M3 5h4v1H3V5zm0 3h4V7H3v1zm0 2h4V9H3v1zm11-5h-4v1h4V5zm0 2h-4v1h4V7zm0 2h-4v1h4V9zm2-6v9c0 .55-.45 1-1 1H9.5l-1 1-1-1H2c-.55 0-1-.45-1-1V3c0-.55.45-1 1-1h5.5l1 1 1-1H15c.55 0 1 .45 1 1zm-8 .5L7.5 3H2v9h6V3.5zm7-.5H9.5l-.5.5V12h6V3z" /></svg>
                                README.md
                            </h3>
                            <article class="markdown-body entry-content" itemprop="text"></article>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="footer container-lg px-3">
        <div class="position-relative d-flex flex-justify-between py-6 mt-6 f6 text-gray border-top border-gray-light">
            <a href="#" aria-label="Homepage" class="footer-octicon" title="">
                <img alt="" class="profile-image" src="~/Assets/images/logo.png">
            </a>
        </div>
    </div>
</body>
</html>